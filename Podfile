# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

target 'Audio_APP_WebRTC' do
  # Use dynamic frameworks
  use_frameworks!

  # Add Google WebRTC pod
  pod 'GoogleWebRTC'

  # Include test targets
  target 'Audio_APP_WebRTCTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Audio_APP_WebRTCUITests' do
    inherit! :search_paths
    # Pods for UI testing
  end

end
